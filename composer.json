{
	"name": "jweiland/mediapool",
	"type": "typo3-cms-extension",
	"description": "Embed your favorite YouTube Videos and Playlists. Import description, title and more by just pasting the YouTube link.",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"typo3",
		"TYPO3 CMS",
		"mediapool",
		"youtube",
		"gallery"
	],
	"homepage": "https://jweiland.net",
	"authors": [
		{
			"name": "Stefan Froemken",
			"email": "projects@jweiland.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "projects@jweiland.net",
		"issues": "https://github.com/jweiland-net/mediapool/issues",
		"source": "https://github.com/jweiland-net/mediapool"
	},
	"require": {
		"typo3/cms-core": "^11.5.40 || ^12.4.25"
	},
	"require-dev": {
		"ergebnis/composer-normalize": "^2.44",
		"friendsofphp/php-cs-fixer": "^3.14",
		"phpstan/phpdoc-parser": "^1.33",
		"phpunit/phpunit": "^9.6",
		"typo3/coding-standards": "^0.6",
		"typo3/testing-framework": "^7.1"
	},
	"replace": {
		"typo3-ter/mediapool": "self.version"
	},
	"autoload": {
		"psr-4": {
			"JWeiland\\Mediapool\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"JWeiland\\Mediapool\\Tests\\": "Tests"
		}
	},
	"config": {
		"allow-plugins": {
			"ergebnis/composer-normalize": true,
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		},
		"bin-dir": ".Build/bin",
		"sort-packages": true,
		"vendor-dir": ".Build/vendor"
	},
	"extra": {
		"typo3/cms": {
			"app-dir": ".Build",
			"extension-key": "mediapool",
			"web-dir": ".Build/public"
		}
	}
}
